{
  "name": "@apps/crm-client",
  "version": "1.0.0",
  "author": "Simon Stranks <sstranks0@gmail.com> <https://github.com/SStranks>",
  "license": "ISC",
  "type": "module",
  "main": "./src/index.tsx",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "last 2 versions"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "clean": "rm -rf dist",
    "codegen": "graphql-codegen --config codegen.ts",
    "codegen:watch": "graphql-codegen --config codegen.ts --watch",
    "start": "webpack-dev-server --config ./webpack/webpack.dev.js",
    "build": "webpack --config ./webpack/webpack.prod.js",
    "build:serve": "http-server ./dist --brotli",
    "build:watch": "webpack --config ./webpack.dev.js --watch",
    "build:stats": "bundle-stats --html -d ./webpack/stats ./webpack/stats/build-stats.json",
    "build:stats:serve": "http-server ./webpack/stats -o bundle-stats.html",
    "vitest:test": "vitest run --config ./vitest.config.ts",
    "vitest:watch": "vitest watch --config ./vitest.config.ts",
    "vitest:list": "vitest list --filesOnly --config ./vitest.config.ts",
    "jest:test": "jest --config ../../../jest.config.js --selectProjects CRM-Client --listTests",
    "jest:coverage": "jest --coverage --config ../../../jest.config.js --selectProjects CRM-Client",
    "jest:watch": "jest --watch --config ../../../jest.config.js --selectProjects CRM-Client",
    "cypress:open": "cypress open",
    "cypress:ui": "start-server-and-test start http://localhost:3000 cypress:run",
    "cypress:e2e": "cypress run",
    "cypress:component": "cypress run --component",
    "storybook": "storybook dev -p 6006 --disable-telemetry",
    "build-storybook": "storybook build --disable-telemetry",
    "sassdoc:build": "sassdoc ./src --dest ./.sassdoc",
    "sassdoc:serve": "http-server ./.sassdoc",
    "dotenv:lint": "docker run --rm -v \"$(pwd)\":/app -w /app dotenvlinter/dotenv-linter:3.3.0 dotenv-linter .env* -r"
  },
  "dependencies": {
